{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "./dist",
    "noEmit": true,
    "types": ["vite/client", "jest", "node", "typia"],
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ],
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strict": true
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
