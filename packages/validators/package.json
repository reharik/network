{
  "name": "@network/validators",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist src/_typia && npm run gen && tsc -p tsconfig.build.json",
    "dev": "concurrently -k -n typia,tsc \"chokidar 'src/**/*.ts' -c 'npm run gen'\" \"tsc -w -p tsconfig.build.json\"",
    "gen": "typia generate --input src/registry --output src/_typia --project tsconfig.build.json"
  },
  "dependencies": {
    "@network/contracts": "*",
    "typia": "^9.7.2"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  }
}
